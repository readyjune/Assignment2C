<Window x:Class="Client.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Client"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox Name="PythonCodeInput" Margin="0, 160, 0, 0" Width="400" Height="50" VerticalAlignment="Top" />
        <Button Content="Browse Python Code" Margin="300, 160, 0, 0" Width="100" Height="50" VerticalAlignment="Top" Click="BrowsePythonCodeButton_Click" />
        <Button Content="Submit" Margin="0,250,0,0" Width="100" Height="50" VerticalAlignment="Top" Click="SubmitButton_Click"/>
        
    </Grid>
</Window>
